{
  "name": "wix-rich-content-fullscreen",
  "version": "8.46.19-loadable",
  "author": {
    "name": "Shaul Goral",
    "email": "shaulg@wix.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix/ricos.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wix/ricos/issues"
  },
  "homepage": "https://github.com/wix/ricos/tree/master/packages/fullscreen",
  "files": [
    "dist",
    "libs",
    "viewer"
  ],
  "main": "dist/module.cjs.js",
  "types": "dist/src/index.d.ts",
  "module": "dist/module.js",
  "style": "dist/styles.min.scss",
  "scripts": {
    "clean": "rm -rf dist/",
    "prebuild": "npm run clean",
    "build:rollup": "rollup --environment MODULE_NAME:fullscreen,EXTRACT_CSS:false -c ../../../rollup.config.js",
    "build": "npm run build:rollup && node ../../../scripts/cssFix.js",
    "build:dev": "cross-env NODE_ENV=development npm run build",
    "build:analyze:editor": "npm run build -- --environment MODULE_ANALYZE_EDITOR  && open stats.html",
    "build:analyze:viewer": "npm run build -- --environment MODULE_ANALYZE_VIEWER && open stats.html",
    "watch": "cross-env NODE_ENV=development MODULE_WATCH=1 BUILD_CJS=1 npm run build:rollup -- --watch.chokidar",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,d.ts}'",
    "test": "jest --verbose"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.2.0",
    "classnames": "^2.0.0",
    "lodash": "^4.0.0",
    "prop-types": "^15.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "fscreen": "1.0.2",
    "wix-rich-content-common": "8.46.19-loadable",
    "wix-rich-content-plugin-gallery": "8.46.19-loadable"
  },
  "unpkg": true,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "jest": {
    "globals": {
      "NODE_ENV": "test"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.jsx?$": "<rootDir>/../../../babel.jest.monorepo.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
